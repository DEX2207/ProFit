<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - ProFit</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css"/>
    <link rel="stylesheet" href="~/css/shared.css" asp-append-version="true"/>
</head>
<body>
<div id="header-top">
    <div class="logo"> ProFit </div>
    <div class="button-header">
        <button class="button">Войти</button>
        <button class="button">Зарегистрироваться</button>
    </div>
</div>
<img class="img-header" src="~/images/first people on ProFit.png" alt="Фитнес-центр ProFit">
<div id="header-bottom">
    <div class="menu">
        <ul>
            <li><a href="#home">Главная</a></li>
            <li><a href="#contact">Контакты</a></li>
            <li><a href="#services">Услуги</a></li>
        </ul>
    </div>
</div>
<div class="container">
        @RenderBody()
</div>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/shared_script.js" asp-append-version="true"></script>
</body>
</html>